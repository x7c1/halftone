{
  "name": "halftone",
  "version": "0.0.1",
  "main": "index.js",
  "config": {
    "prettier_targets": [
      "src-ui/**/*.js",
      "conf/**/*.js"
    ]
  },
  "scripts": {
    "prettier": "prettier --config conf/prettier.config.js",
    "prettier:check": "npm run prettier -- --check $npm_package_config_prettier_targets",
    "prettier:write": "npm run prettier -- --write $npm_package_config_prettier_targets",
    "webpack": "webpack",
    "webpack:build": "webpack --config conf/webpack.config.js"
  },
  "repository": "git@github.com:x7c1/halftone.git",
  "author": "x7c1",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^5.2.0",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
